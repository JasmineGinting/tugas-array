public class tugasArray1 {

    public static void main(String[] args) {

        int bil [] = new int [5];
        bil [0] = 1;
        bil [1] = 2;
        bil [2] = 3;
        bil [3] = 4;
        bil [4] = 5;
        for (int i = 0; i < bil.length; i++) {
            System.out.println(bil[i]);
        }
    }
}
