public class tugasArray2 {

    public static void main(String[] args) {
        
        int bil[] = new int [8];
        bil [0] = 1;
        bil [1] = 2;
        bil [2] = 3;
        bil [3] = 4;
        bil [4] = 5;
        bil [5] = 6;
        bil [6] = 7;
        bil [7] = 8;
        for (int i = 0; i < bil.length; i++) {
            System.out.println(bil [i]);
        }
    } 
}
